{
    "compilerOptions": {
      "target": "es2016",     
      "module": "commonjs",                            
      "rootDir": "./src",                       
      "outDir": "./dist",       
      "noEmit": true, //Temporaly enabled until every dist files are converted to typescript
      "noEmitOnError": true,                         
      "esModuleInterop": true,                           
      "forceConsistentCasingInFileNames": true,         
      "strict": true,                         
      "skipLibCheck": true    
    }
  }